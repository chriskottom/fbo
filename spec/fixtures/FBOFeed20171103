<tr style="mso-yfti-irow: 0; mso-yfti-firstrow: yes">
<td style="width: 121.5pt; padding-bottom: 0in; padding-top: 0in; padding-left: 5.4pt; padding-right: 5.4pt; mso-border-alt: solid windowtext .5pt; border: windowtext 1pt solid;" width="162" valign="top">
<p><span style="FONT-SIZE: 11pt">Part Number</span></p>
</td>
<td style="border-top: windowtext 1pt solid; border-right: windowtext 1pt solid; width: 1.75in; border-bottom: windowtext 1pt solid; padding-bottom: 0in; padding-top: 0in; padding-left: 5.4pt; border-left: medium none; padding-right: 5.4pt; mso-border-alt: solid windowtext .5pt; mso-border-left-alt: solid windowtext .5pt;" width="168" valign="top">
<p><span style="FONT-SIZE: 11pt">Nomenclature</span></p>
</td>
<td style="border-top: windowtext 1pt solid; border-right: windowtext 1pt solid; width: 119.7pt; border-bottom: windowtext 1pt solid; padding-bottom: 0in; padding-top: 0in; padding-left: 5.4pt; border-left: medium none; padding-right: 5.4pt; mso-border-alt: solid windowtext .5pt; mso-border-left-alt: solid windowtext .5pt;" width="160" valign="top">
